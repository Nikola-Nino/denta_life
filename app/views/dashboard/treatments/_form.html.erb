<%=form_with model: [:dashboard,@treatment] do |f|%>
  <div class=" items-center mt-2 sm:mt-8 flex">
      <button class="bg-blue-600 text-gray-100 rounded-md h-8 sm:h-auto sm:rounded-lg w-20 sm:w-52 p-1 text-xs sm:text-md sm:p-3 ">
        Create treatment </button>
        <%=link_to 'Cancel',dashboard_treatments_path, class:"bg-blue-600 text-center text-gray-100 rounded-md h-8 sm:h-auto sm:rounded-lg w-20 sm:w-52 p-1 text-xs sm:text-md sm:p-3 " %>
      </div>
    <%= f.file_field :images, :multiple => true %>
  <% end %>

